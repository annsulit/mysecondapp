<h1>Listing mike_ann_kids</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Age</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mike_ann_kids.each do |mike_ann_kid| %>
      <tr>
        <td><%= mike_ann_kid.first_name %></td>
        <td><%= mike_ann_kid.last_name %></td>
        <td><%= mike_ann_kid.age %></td>
        <td><%= link_to 'Show', mike_ann_kid %></td>
        <td><%= link_to 'Edit', edit_mike_ann_kid_path(mike_ann_kid) %></td>
        <td><%= link_to 'Destroy', mike_ann_kid, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mike ann kid', new_mike_ann_kid_path %>
